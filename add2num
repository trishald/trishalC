#include<stdio.h>
int input(int *a,int *b)
{
  printf("Enter a\n");
  scanf("%d",a);
  printf("Enter b\n");
  scanf("%d",b);
}
int compute(int a, int b)
{
  int sum;
  sum=a+b;
  return sum;
}
int output(int a,int b,int c)
{
  printf("%d+%d=%d",a,b,c);
}
void main()
{
  int x,y,z;
  input(&x,&y);
  z=compute(x,y);
  output(x,y,z);
}
